{
  "name": "pomodoro",
  "private": true,
  "version": "3.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MkMan/pomodoro"
  },
  "scripts": {
    "build": "vite build --base /pomodoro",
    "e2e:open": "concurrently -k -s first \"npm start\" \"cypress open\"",
    "e2e:run": "concurrently -k -s first \"npm start\" \"cypress run\"",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint:check": "eslint ./src && markdownlint .",
    "lint:fix": "eslint ./src --fix && markdownlint . --fix",
    "prepare": "husky install",
    "preview": "vite preview",
    "sanity-check": "npm run lint:check && npm run format:check && npm run ts:check && npm test && npm run e2e:run",
    "start": "vite --open --port 3000 --base /pomodoro",
    "test:watch": "vitest watch --coverage",
    "test": "vitest run --coverage",
    "ts:check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fontsource/raleway": "^5.0.8",
    "@thisbeyond/solid-dnd": "^0.7.4",
    "@vanilla-extract/css": "^1.13.0",
    "solid-icons": "^1.0.12",
    "solid-js": "^1.7.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@solidjs/testing-library": "^0.8.4",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitest/coverage-c8": "^0.31.1",
    "concurrently": "8.2.1",
    "cypress": "^12.12.0",
    "eslint": "8.49.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-perfectionist": "^2.0.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "6.0.1",
    "eslint-plugin-vitest": "^0.2.3",
    "gh-pages": "^6.0.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "14.0.1",
    "markdownlint": "0.31.0",
    "markdownlint-cli": "0.36.0",
    "prettier": "3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "21.1.1",
    "storybook": "^7.4.0",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
    "typescript": "5.2.2",
    "vite": "4.3.8",
    "vite-plugin-pwa": "^0.15.0",
    "vite-plugin-solid": "^2.7.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.31.1",
    "wait-on": "^7.0.1",
    "workbox-window": "^6.5.4"
  },
  "engines": {
    "node": "18.13.0",
    "pnpm": "8.5.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md,json,html}": "prettier --write",
    "*.md": "markdownlint --fix"
  }
}
