{
  "name": "pomodoro",
  "private": true,
  "version": "1.8.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MkMan/pomodoro"
  },
  "scripts": {
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "build": "vite build --base /pomodoro",
    "e2e:open": "concurrently -k -s first \"npm start\" \"cypress open\"",
    "e2e:run": "concurrently -k -s first \"npm start\" \"cypress run\"",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint:check": "eslint ./src && markdownlint .",
    "lint:fix": "eslint ./src --fix && markdownlint . --fix",
    "loki:update": "concurrently -k -s first \"npm run storybook -- --no-open\" \"wait-on http-get://localhost:6006/ -i 5000 && loki update\"",
    "loki:test": "concurrently -k -s first \"npm run storybook -- --no-open\" \"wait-on http-get://localhost:6006/ -i 5000 && loki test\"",
    "prepare": "husky install",
    "preview": "vite preview",
    "sanity-check": "npm run lint:check && npm run format:check && npm run ts:check && npm test && npm run e2e:run",
    "start": "vite --port 3000",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "test:watch": "vitest watch --coverage",
    "test": "vitest run --coverage",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "17.4.0",
    "@commitlint/config-conventional": "17.4.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-react": "3.0.1",
    "@vitest/coverage-c8": "^0.26.3",
    "babel-loader": "^8.3.0",
    "concurrently": "7.6.0",
    "cypress": "^12.3.0",
    "eslint": "8.31.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-testing-library": "5.9.1",
    "gh-pages": "^4.0.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "13.1.0",
    "loki": "^0.31.0",
    "markdownlint": "0.27.0",
    "markdownlint-cli": "0.33.0",
    "prettier": "2.8.2",
    "semantic-release": "20.0.2",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "0.26.3",
    "wait-on": "^7.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md,json,html}": "prettier --write",
    "*.md": "markdownlint --fix"
  }
}
